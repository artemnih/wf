{
  "name": "compute",
  "version": "0.0.0",
  "private": true,
  "contributors": "https://github.com/LabShare/compute-api/graphs/contributors",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/LabShare/compute-api.git"
  },
  "bugs": {
    "url": "https://github.com/LabShare/compute-api/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "lsc": {
    "loopbackApi": {
      "name": "compute"
    },
    "packageDependencies": {
      "@labshare/services": "@labshare/services"
    }
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run eslint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t compute .",
    "docker:run": "docker run -p 3000:3000 -d compute",
    "migrate": "node ./dist/migrate",
    "coverage": "nyc npm run test",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "extends": "@labshare/semantic-release-config",
    "branch": "master"
  },
  "author": "LabShare",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@labshare/services": "^4.3.0",
    "@labshare/services-auth": "^3.3.1",
    "@labshare/services-cache": "^2.1.1",
    "@labshare/services-logger": "^1.1.6",
    "@labshare/services-notifications": "^1.0.2",
    "@loopback/boot": "^2.3.3",
    "@loopback/context": "^3.9.0",
    "@loopback/core": "^2.8.0",
    "@loopback/openapi-v3": "^3.4.3",
    "@loopback/repository": "^2.7.0",
    "@loopback/rest": "^5.1.1",
    "@loopback/rest-explorer": "^2.2.4",
    "@loopback/service-proxy": "^2.3.2",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "loopback-connector-mongodb": "^5.2.3",
    "p-event": "^4.1.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@labshare/semantic-release-config": "^1.1.2",
    "@loopback/build": "^5.4.3",
    "@loopback/eslint-config": "^8.0.1",
    "@loopback/testlab": "^3.1.7",
    "@types/express": "^4.17.4",
    "@types/node": "^10.17.26",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^7.2.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^7.0.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "npm": "^6.14.5",
    "nyc": "^15.1.0",
    "semantic-release": "^17.0.4",
    "snyk": "^1.305.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.8.2",
    "typescript": "~3.9.5"
  },
  "snyk": true
}
