{
  "name": "@Polus/argo-compute",
  "version": "0.2.0",
  "private": true,
  "description": "Argo-Compute Driver",
  "main": "index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "lb-clean dist *.tsbuildinfo",
    "pretest": "npm run clean && npm run build",
    "test": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:unit": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/unit\"",
    "test:argoapi": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/argoApi\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t argo-compute .",
    "docker:run": "docker run -p 3000:3000 -d argo-compute",
    "prestart": "npm run build",
    "start": "ts-node ./index.ts",
    "start:dev": "ts-node-dev --watch .env, src ./index.ts",
    "prepublishOnly": "npm run test"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "tslib": "^2.6.2",
    "axios": "1.5.0",
    "express-jwt": "^8.4.1",
    "jwks-rsa": "^3.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@types/cors": "^2.8.14",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.4.9"
  }
}