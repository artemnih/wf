{
  "name": "compute",
  "version": "0.0.4",
  "private": true,
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:unit": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/unit\"",
    "test:regression": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/regression\"",
    "posttest": "npm run eslint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t compute .",
    "docker:run": "docker run -p 3000:3000 -d compute",
    "migrate": "node ./dist/migrate",
    "coverage": "nyc npm run test",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "config": "^3.3.7",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "p-event": "^4.1.0",
    "tslib": "^2.3.1",
    "axios": "1.5.0",
    "express-jwt": "^8.4.1",
    "mongoose": "^7.5.0",
    "jwks-rsa": "^3.0.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/express": "^4.17.4",
    "@types/node": "^10.17.17",
    "@types/cors": "^2.8.5",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^10.6.0",
    "typescript": "^4.5.5"
  }
}