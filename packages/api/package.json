{
  "name": "@Polus/compute-api",
  "version": "0.0.4",
  "private": true,
  "description": "",
  "main": "index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rm rf dist",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:unit": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/unit\"",
    "test:regression": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/regression\"",
    "posttest": "npm run eslint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t compute .",
    "docker:run": "docker run -p 3000:3000 -d compute",
    "prestart": "npm run build",
    "start": "ts-node ./index.ts",
    "start:dev": "ts-node-dev --watch .env, src ./index.ts",
    "prepublishOnly": "npm run test"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "tslib": "^2.6.2",
    "axios": "1.5.0",
    "express-jwt": "^8.4.1",
    "mongoose": "^7.5.0",
    "jwks-rsa": "^3.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@types/cors": "^2.8.14",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vite": "^4.4.9"
  }
}