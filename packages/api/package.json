{
  "name": "compute",
  "version": "0.0.4",
  "private": true,
  "contributors": "https://github.com/LabShare/compute-api/graphs/contributors",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/LabShare/compute-api.git"
  },
  "bugs": {
    "url": "https://github.com/LabShare/compute-api/issues"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "lsc": {
    "loopbackApi": {
      "name": "compute"
    },
    "packageDependencies": {
      "@labshare/services": "@labshare/services"
    }
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:unit": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/unit\"",
    "test:regression": "npm run pretest && lb-mocha --allow-console-logs \"dist/__tests__/regression\"",
    "posttest": "npm run eslint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t compute .",
    "docker:run": "docker run -p 3000:3000 -d compute",
    "migrate": "node ./dist/migrate",
    "coverage": "nyc npm run test",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "release": {
    "extends": "@labshare/semantic-release-config",
    "branch": "master"
  },
  "author": "LabShare",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@labshare/services-auth": "^3.5.7",
    "@labshare/services-cache": "^2.2.6",
    "@labshare/services-logger": "^1.3.4",
    "@labshare/services-notifications": "^1.1.5",
    "@loopback/boot": "^3.4.4",
    "@loopback/context": "^3.18.0",
    "@loopback/core": "^2.18.0",
    "@loopback/openapi-v3": "^5.3.1",
    "@loopback/repository": "^3.7.3",
    "@loopback/rest": "^9.3.1",
    "@loopback/rest-explorer": "^4.1.1",
    "@loopback/service-proxy": "^3.2.4",
    "config": "^3.3.7",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "loopback-connector-mongodb": "^5.6.0",
    "p-event": "^4.1.0",
    "tslib": "^2.3.1",
    "axios": "1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@labshare/semantic-release-config": "^1.1.2",
    "@loopback/build": "^8.1.1",
    "@loopback/eslint-config": "^12.0.3",
    "@loopback/testlab": "^4.1.1",
    "@types/express": "^4.17.4",
    "@types/node": "^10.17.17",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^10.6.0",
    "typescript": "^4.5.5"
  },
  "snyk": true
}