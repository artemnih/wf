{
  "name": "ome-to-zarr-scatter",
  "version": "0.0.1",
  "inputs": {
    "filePatternArray": "string[]",
    "inpDir": "Directory",
    "outDir": "Directory"
  },
  "outputs": {
    "ome2zarroutDir": {
      "type": "Directory[]",
      "outputSource": [
        "ome2zarr/ome2zarroutDir"
      ]
    },
    "ome2zarrStdOut": {
      "type": "File[]",
      "outputSource": [
        "ome2zarr/ome2zarrStdOut"
      ]
    },
    "ome2zarrStdErr": {
      "type": "File[]",
      "outputSource": [
        "ome2zarr/ome2zarrStdErr"
      ]
    }
  },
  "steps": {
    "ome2zarr": {
      "run": "plugin:ome2zarr:0.2.1",
      "scatter": "filePattern",
      "in": {
        "filePattern": "filePatternArray",
        "inpDir": "inpDir",
        "outDir": "outDir"
      },
      "out": [
        "ome2zarroutDir",
        "ome2zarrStdOut",
        "ome2zarrStdErr"
      ]
    }
  }
}