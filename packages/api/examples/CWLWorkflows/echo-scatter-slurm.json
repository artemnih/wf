{
	"name": "echo-scatter-slurm",
	"driver": "slurm",
	"inputs": {
		"helloArray": "string[]"
	},
	"outputs": {
		"echoStdOut": {
			"type": "File[]",
			"outputSource": [
				"echo/echoStdOut"
			]
		},
		"echoStdErr": {
			"type": "File[]",
			"outputSource": [
				"echo/echoStdErr"
			]
		}
	},
	"cwlJobInputs": {
		"helloArray": [
			"hello",
			"nihao",
			"hola"
		]
	},
	"steps": {
		"echo": {
			"run": {
				"cwlVersion": "v1.0",
				"id": "echo",
				"class": "CommandLineTool",
				"requirements": {
					"DockerRequirement": {
						"dockerPull": "busybox"
					}
				},
				"baseCommand": [
					"echo"
				],
				"stdout": "echo.out",
				"stderr": "echo.err",
				"inputs": {
					"message": {
						"type": "string",
						"inputBinding": {
							"position": 1
						}
					}
				},
				"outputs": {
					"echoStdOut": {
						"type": "stdout"
					},
					"echoStdErr": {
						"type": "stderr"
					}
				}
			},
			"scatter": "message",
			"in": {
				"message": "helloArray"
			},
			"out": [
				"echoStdOut",
				"echoStdErr"
			]
		}
	}
}