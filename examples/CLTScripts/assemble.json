{
	"id": "6140d6550e01180bcd90c012",
	"cwlScript": {
		"cwlVersion": "v1.0",
		"id": "assemble",
		"class": "CommandLineTool",
		"requirements": {
			"DockerRequirement": {
				"dockerPull": "kevinpatrickhannon/polus-assemble:test"
			},
			"InitialWorkDirRequirement": {
				"listing": [{
					"entry": "$(inputs.outDir)",
					"writable": true
				}]
			}
		},
		"baseCommand": ["python3", "/opt/executables/main.py"],
		"stdout": "assemble.out",
		"stderr": "assemble.err",
		"inputs": {
			"stitchPath": {
				"type": "Directory",
				"inputBinding": {
					"prefix": "--stitchPath"
				}
			},
			"imgPath": {
				"type": "Directory",
				"inputBinding": {
					"prefix": "--imgPath"
				}
			},
			"timesliceNaming": {
				"type": "boolean",
				"inputBinding": {
					"prefix": "--timesliceNaming"
				}
			},
			"outDir": {
				"type": "Directory",
				"inputBinding": {
					"prefix": "--outDir"
				}
			}
		},
		"outputs": {
			"assembleOut": {
				"type": "Directory",
				"outputBinding": {
					"glob": "$(inputs.outDir.basename)"
				}
			},
			"assembleStdOut": {
				"type": "stdout"
			},
			"assembleStdErr": {
				"type": "stderr"
			}
		}
	}
}