{
	"name": "ome-zarr-slurm",
	"driver": "slurm",
	"inputs": {
		"filePattern": "string",
		"inpDir": "Directory",
		"outDir": "Directory"
	},
	"outputs": {
		"ome2zarroutDir": {
			"type": "Directory",
			"outputSource": "ome2zarr/ome2zarroutDir"
		},
		"ome2zarrStdOut": {
			"type": "File",
			"outputSource": "ome2zarr/ome2zarrStdOut"
		},
		"ome2zarrStdErr": {
			"type": "File",
			"outputSource": "ome2zarr/ome2zarrStdErr"
		}
	},
	"cwlJobInputs": {
		"filePattern": "x24_y16_wx2_wy1_c{c}.ome.tif",
		"inpDir": {
			"class": "Directory",
			"path": "/project/labshare-compute/data/tiff-converter"
		},
		"outDir": {
			"class": "Directory",
			"path": "zarr"
		}
	},
	"steps": {
		"ome2zarr": {
			"run": "plugin:ome2zarr:0.2.1",
			"in": {
				"filePattern": "filePattern",
				"inpDir": "inpDir",
				"outDir": "outDir"
			},
			"out": [
				"ome2zarroutDir",
				"ome2zarrStdOut",
				"ome2zarrStdErr"
			]
		}
	}
}